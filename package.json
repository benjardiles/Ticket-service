{
    "name": "ticket-service",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "debug": "nodemon --exec 'babel-node --inspect=0.0.0.0 src/app.js'",
        "build": "npm run clean && babel ./src --out-dir dist",
        "clean": "rm -rf ./dist && mkdir dist",
        "start": "node ./dist/app.js",
        "dev": "nodemon --exec babel-node ./src/app.js",
        "test": "mocha --require @babel/register ./src/test --recursive",
        "test_dist": "mocha ./dist/test --recursive",
        "coverage": "nyc mocha ./src/test --recursive; rm -R ./.nyc_output",
        "lint": "eslint ./src --quiet",
        "pretty_check": "npx prettier --check ./src",
        "pretty_write": "npx prettier --write ./src",
        "full_clean_install": "rm -rf ./node_modules && rm package-lock.json && npm i && make install"
    },
    "dependencies": {
        "@babel/runtime": "^7.12.1",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "express": "^4.18.2",
        "helmet": "^4.1.1",
        "http-status-codes": "^2.1.4",
        "joi": "^17.3.0",
        "morgan": "~1.9.1",
        "mysql2": "^3.12.0",
        "pg": "^8.13.3",
        "postgres": "^3.4.5",
        "sequelize": "^6.37.5",
        "uuid": "^8.3.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.1",
        "@babel/core": "^7.12.3",
        "@babel/node": "^7.12.1",
        "@babel/register": "^7.12.1",
        "@faker-js/faker": "^9.0.3",
        "@istanbuljs/nyc-config-babel": "^3.0.0",
        "babel-plugin-istanbul": "^6.0.0",
        "babel-preset-airbnb": "^5.0.0",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "eslint": "^7.12.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.14.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^4.3.0",
        "mocha": "^9.2.0",
        "nock": "^13.3.1",
        "nodemon": "^3.0.1",
        "nyc": "^15.1.0",
        "prettier": "2.1.2",
        "proxyquire": "^2.1.3",
        "sinon": "^11.1.2",
        "sinon-chai": "^3.7.0",
        "supertest": "^6.3.3"
    }
}
